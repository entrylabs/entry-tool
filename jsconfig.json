{
    "compilerOptions": {
        "target": "ES6",
        "baseUrl": "./",
        "paths": {
            "@hoc/*": ["src/hoc/*"],
            "@actions/*": ["src/actions/*"],
            "@actions/scss/*": ["src/actions/scss/*"],
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@utils/*": ["src/utils/*"],
            "@reducers/*": ["src/reducers/*"],
            "@contexts/*": ["src/contexts/*"]
        }
    },
    "exclude": ["node_modules", "**/node_modules/*", "dist", "component"]
}
